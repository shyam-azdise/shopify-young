<div class="slider-area section-padding-1" id="section-{{ section.id }}">
  <div class="container{% if section.settings.fullwidth_on %}-fluid{% endif %}">
    <div class="main-slider-active-1 owl-carousel slider-nav-position-3 slider-nav-style-1">
      {%- assign sset = section.settings -%}
      {%- assign header_font = sset.header_font -%}
      {%- assign sub_header_font = sset.sub_header_font -%}
      {%- assign paragraph_font = sset.paragraph_font -%}
      {%- assign button_font = sset.button_font -%}

      {% for block in section.blocks %} 
      {%- case block.type -%}
      {%- when 'slide' -%}
      <div class="single-main-slider slider-animated-1">
        <div class="row">
          <div class="col-12">
            <div class="furniture-slider-wrap">
              <img src="{% if block.settings.slider_img %}{{ block.settings.slider_img | img_url: 'master' }}{% else %}https://via.placeholder.com/1154x930 {% endif %}" class="lazyload" alt="banner">
              <div class="main-slider-content-3-wrap">
                <div class="main-slider-content-3 ssc-{{ block.id }}">
                  {%if block.settings.slider_sub_title != '' %}
                  <span class="animated">{{ block.settings.slider_sub_title }}</span>
                  {%endif%}
                  {%if block.settings.slider_title != '' %}
                  <h1 class="animated">{{ block.settings.slider_title }}</h1>
                  {%endif%}
                  {%if block.settings.slider_des != '' %}
                  <p class="animated">{{ block.settings.slider_des }}</p>
                  {%endif%}
                  {% if block.settings.slider_button != '' %}
                  <div class="slider-btn-1 btn-hover">
                    <a class="animated slider-btn-1-border-1 slider-btn-1-ptb theme-color-yellow-2-hover" href="{{ block.settings.slider_button_url }}">{{ block.settings.slider_button }} </a>
                  </div>
                  {% endif %}
                  
                  
                  <style>

                    {% if block.settings.content_color_enable %}

                    .furniture-slider-wrap .main-slider-content-3-wrap .main-slider-content-3.ssc-{{ block.id }} h1 {
                      color: {{ block.settings.slider_title_color }};
                    }
                    .furniture-slider-wrap .main-slider-content-3-wrap .main-slider-content-3.ssc-{{ block.id }} span {
                      color: {{ block.settings.slider_sub_title_color }};
                    }
                    .furniture-slider-wrap .main-slider-content-3-wrap .main-slider-content-3.ssc-{{ block.id }} p {
                      color: {{ block.settings.slider_discount_color }};
                    }
                    .furniture-slider-wrap .main-slider-content-3-wrap .main-slider-content-3.ssc-{{ block.id }} a {
                      color: {{ block.settings.slider_button_color }};
                      background: {{ block.settings.slider_button_bg_color }};
                    }

                    .furniture-slider-wrap .main-slider-content-3-wrap .main-slider-content-3.ssc-{{ block.id }} a:hover {
                      color: {{ block.settings.slider_button_color_hov }};
                    }
                    .btn-hover a.bg-black-hover::after {
                      background-color: {{ block.settings.slider_button_bg_hover_color }};
                    }

                    .owl-item.active .slider-animated-1 .ssc-{{ block.id }} h1, .owl-item.active .slider-animated-1  .ssc-{{ block.id }} h3, .owl-item.active .slider-animated-2 .ssc-{{ block.id }} p, .owl-item.active .slider-animated-2 .ssc-{{ block.id }} a {
                      -webkit-animation-name: {{block.settings.slider_animation_animation}};
                      animation-name: {{block.settings.slider_animation_animation}};
                    }

                    {% endif %}

                  </style>
                  
                  
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      {%- endcase -%}
      {% endfor %}
    </div>
  </div>
</div>





{%- style -%}
  #section-{{ section.id }}{
    {%- for block in section.blocks -%}
    {%- case block.type -%}
    {%- when 'section_margin' -%}
    margin-top: {{ block.settings.section_margin_top }}px;
    margin-bottom: {{ block.settings.section_margin_bottom }}px;
    {%- when 'section_padding' -%}
    padding-top: {{ block.settings.section_padding_top }}px;
    padding-bottom: {{ block.settings.section_padding_bottom }}px;
    {%- when 'section_background' -%}    
    background: {{ block.settings.section_bg_color }} 
      {%- if block.settings.section_bg_image -%}url("{{ block.settings.section_bg_image | img_url: 'master' }}"){%- endif -%} no-repeat scroll center center / cover;
    {%- endcase -%}
    {%- endfor -%}
  }
  @media (min-width: 768px) and (max-width: 991px) {
    #section-{{ section.id }}{
      {%- for block in section.blocks -%}
      {% case block.type %}
      {%- when 'section_margin' -%}
      margin-top: {{ block.settings.section_margin_top_md }}px;
      margin-bottom: {{ block.settings.section_margin_bottom_md }}px;
      {%- when 'section_padding' -%}
      padding-top: {{ block.settings.section_padding_top_md }}px;
      padding-bottom: {{ block.settings.section_padding_bottom_md }}px;
      {%- endcase -%}
      {%- endfor -%}
    }
  }
  @media (max-width: 767px) {
    #section-{{ section.id }}{
      {%- for block in section.blocks -%}
      {%- case block.type -%}
      {%- when 'section_margin' -%}
      margin-top: {{ block.settings.section_margin_top_xs }}px;
      margin-bottom: {{ block.settings.section_margin_bottom_xs }}px;
      {%- when 'section_padding' -%}
      padding-top: {{ block.settings.section_padding_top_xs }}px;
      padding-bottom: {{ block.settings.section_padding_bottom_xs }}px;
      {%- endcase -%}
      {%- endfor -%}
    }
  }
{% if section.settings.slider_content_enable %}

  {{ header_font | font_face }}
  {{ sub_header_font | font_face }}
  {{ paragraph_font | font_face }}
  {{ button_font | font_face }}


  .text-right {
      text-align: right!important;
  }
  .main-slider-content-3 h1 {
    font-family: {{ header_font.family }}, {{ header_font.fallback_families }};
    font-size: {{ section.settings.title_font_size }}px;
    line-height: normal;
    font-style: {{ header_font.style }};
  }
  .main-slider-content-3 span {
	font-family: {{ sub_header_font.family }}, {{ sub_header_font.fallback_families }};
    font-size: {{ section.settings.sub_title_font_size }}px;
    line-height: normal;
  }
  .main-slider-content-3 p {
    font-family: {{ paragraph_font.family }}, {{ paragraph_font.fallback_families }};
    font-size: {{ section.settings.content_font_size }}px;
    line-height: normal;
  }
  .btn-style-1 a {
	  font-family: {{ button_font.family }}, {{ button_font.fallback_families }};
      font-size: {{ section.settings.button_font_size }}px;
	  line-height: normal;
  }

{%endif%}


{%- endstyle -%}

<script>
  
  /* main slider active 1 */
    $('.main-slider-active-1').owlCarousel({
        loop: true,
        nav: true,
        navText: ['<i class="lnr lnr-arrow-left"></i>', '<i class="lnr lnr-arrow-right"></i>'],
        autoplay: {{ section.settings.slider_auto_play }},
        autoplayTimeout: 5000,
        animateOut: 'fadeOut',
        {%- if settings.rtl_enable -%}
        rtl: true,
        {%- else -%}
        fade: false,
        {%- endif -%}
        animateIn: 'fadeIn',
        item: 1,
        responsive: {
            0: {
                items: 1
            },
            576: {
                items: 1
            },
            768: {
                items: 1
            },
            992: {
                items: 1
            }
        }
    })

</script>

